<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App4.AlunoPage">
    <ContentPage.Content>
        <StackLayout>
            <Entry x:Name="Entry1" Placeholder="Nome" />
            <Entry x:Name="Entry2" Placeholder="Matrícula" />
            <Button x:Name="Button" Text="Cadastrar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>
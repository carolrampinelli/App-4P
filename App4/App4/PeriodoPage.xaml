<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App4.PeriodoPage">
    <ContentPage.Content>
        <StackLayout>
            <Picker x:Name="Picker" Title="Aluno"></Picker>
            <Entry x:Name="Entry" Keyboard="Numeric" Placeholder="Periodo" />
            <Label x:Name="Label" IsVisible="True" Text="" TextColor="Black" />
            <Button x:Name="Button" Text="Salvar"
                HorizontalOptions="Center"
                Clicked="OnButtonClicked" />
        </StackLayout>
    </ContentPage.Content>
</ContentPage>